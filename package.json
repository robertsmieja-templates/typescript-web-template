{
  "author": "Robert Smieja",
  "description": "Template repository for a Typescript-based Node.js project",
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.32",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "source-map-loader": "^2.0.1",
    "ts-loader": "^8.0.17",
    "typescript": "^4.2.3",
    "webpack": "^5.24.3",
    "webpack-cli": "^4.5.0",
    "webpack-merge": "^5.7.3"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ]
    }
  },
  "keywords": [
    "typescript",
    "nodejs",
    "template",
    "babel",
    "webpack"
  ],
  "license": "MIT",
  "lint-staged": {
    "src/*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ],
    "*.{js,ts,jsx,tsx,json,md}": [
      "prettier --write"
    ]
  },
  "main": "lib/index.js",
  "name": "typescript-template",
  "private": true,
  "scripts": {
    "build": "npm run-script build:prod",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:dev": "cross-env NODE_ENV=development webpack --config webpack.config.dev.js",
    "clean": "rimraf dist/ lib/",
    "lint": "eslint ./src/**/*",
    "lint:staged": "lint-staged",
    "lint:fix": "eslint --fix ./src/**/*",
    "prettier": "prettier --write '*.{js,ts,jsx,tsx,json,md}'",
    "start": "run-s build:dev start:dev",
    "start:dev": "node dist/bundle.js",
    "start:prod": "serve dist/",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "version": "0.1.0",
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "@types/react": "^17.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5"
  }
}
